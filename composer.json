{
    "description": "Happy Inc Worker",
    "name": "happy-inc/worker",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Happy Inc Tech Team",
            "homepage": "https://github.com/happy-inc-tech"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-pcntl": "*",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "ext-posix": "*",
        "friendsofphp/php-cs-fixer": "^2.17",
        "icanhazstring/composer-unused": "^0.7",
        "maglnet/composer-require-checker": "^2.1",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-phpunit": "^0",
        "vimeo/psalm": "^4.2.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "HappyInc\\Worker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HappyInc\\Worker\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
