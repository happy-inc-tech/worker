{
    "description": "Happy Inc Worker",
    "name": "happy-inc/worker",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Happy Inc Tech Team",
            "homepage": "https://github.com/happy-inc-tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pcntl": "*",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "ext-posix": "*",
        "friendsofphp/php-cs-fixer": "^3.4.0",
        "icanhazstring/composer-unused": "^0.7.12",
        "maglnet/composer-require-checker": "^4.0.0",
        "phpunit/phpunit": "^9.5.11",
        "psalm/plugin-phpunit": "~0.16",
        "vimeo/psalm": "^4.18.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "HappyInc\\Worker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HappyInc\\Worker\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
